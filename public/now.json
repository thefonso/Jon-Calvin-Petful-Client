{
    "version": 2,
    "name": "petful-app",
    "alias": "jon-calvin-petful-app",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/favicon.ico$",
            "dest": "/favicon.ico"
        },
        {
            "src": "^/manifest.json$",
            "dest": "/manifest.json"
        },
        {
            "src": "^/site.webmanifest.json$",
            "dest": "/site.webmanifest.json"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}